    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/outcome_forecast/train.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/outcome_forecast",
            "args": [
                "--workspace=workspace",
                "--epoch=10",
                "--config-file=cfg/train-numpy.yml",
                "--pbar",
                "--brief=testing this",
                "--workers=6"
            ],
            "envFile": "${workspaceFolder}/.env"
        },
    ]
}